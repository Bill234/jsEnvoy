{
  "name": "@jsenvoy/cli",
  "version": "0.0.1",
  "description": "jsEnvoy CLI - A generic command-line interface for executing modular AI agent tools",
  "main": "src/index.js",
  "bin": {
    "jsenvoy": "bin/jsenvoy"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "cli",
    "jsenvoy",
    "ai",
    "tools",
    "command-line"
  ],
  "author": "Anand Sukumaran",
  "license": "MIT",
  "dependencies": {
    "@jsenvoy/core": "^0.0.1",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}